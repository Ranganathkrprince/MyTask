package com.niftem.app.model;

import javax.validation.constraints.NotBlank;
import javax.validation.constraints.NotEmpty;
import javax.validation.constraints.NotNull;

public class HostelRequest {
	
	private Long id;  
	
	@NotEmpty@NotNull@NotBlank(message = "Please provide HosteName")
	private String exName;
	
	@NotEmpty@NotNull@NotBlank(message = "Please provide HostelType")
	private String exHostelType;
	
	@NotEmpty@NotNull@NotBlank(message = "Please provide HostelCode")
	private String exHostelCode;
	
	@NotEmpty@NotNull@NotBlank(message = "Please provide HostelCapacity")
	private String exHostelCapacity;
	
	@NotEmpty@NotNull@NotBlank(message = "Please provide HostelIncharge")
	private String exHostelIncharge;
	
	@NotEmpty@NotNull@NotBlank(message = "Please provide HostelAddress")
	private String exHostelAddress;	
	
	@NotNull(message = "Please provide colleges")
	private Long colleges;
	
	@NotEmpty@NotNull@NotBlank(message = "Please provide Telephone")
	private String exTelephone;
	
	@NotNull(message = "Please provide a exIsActive 0/1")
	private Boolean exIsActive;
	
	private String singleRoom;
	private String doubleRoom;
	private String tripleRoom;
	private Long user;
	public Long getId() {
		return id;
	}
	public String getExName() {
		return exName;
	}
	public String getExHostelType() {
		return exHostelType;
	}
	public String getExHostelCode() {
		return exHostelCode;
	}
	public String getExHostelCapacity() {
		return exHostelCapacity;
	}
	public String getExHostelIncharge() {
		return exHostelIncharge;
	}
	public String getExHostelAddress() {
		return exHostelAddress;
	}
	public Long getColleges() {
		return colleges;
	}
	public String getExTelephone() {
		return exTelephone;
	}
	public Boolean getExIsActive() {
		return exIsActive;
	}
	public String getSingleRoom() {
		return singleRoom;
	}
	public String getDoubleRoom() {
		return doubleRoom;
	}
	public String getTripleRoom() {
		return tripleRoom;
	}
	public Long getUser() {
		return user;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public void setExName(String exName) {
		this.exName = exName;
	}
	public void setExHostelType(String exHostelType) {
		this.exHostelType = exHostelType;
	}
	public void setExHostelCode(String exHostelCode) {
		this.exHostelCode = exHostelCode;
	}
	public void setExHostelCapacity(String exHostelCapacity) {
		this.exHostelCapacity = exHostelCapacity;
	}
	public void setExHostelIncharge(String exHostelIncharge) {
		this.exHostelIncharge = exHostelIncharge;
	}
	public void setExHostelAddress(String exHostelAddress) {
		this.exHostelAddress = exHostelAddress;
	}
	public void setColleges(Long colleges) {
		this.colleges = colleges;
	}
	public void setExTelephone(String exTelephone) {
		this.exTelephone = exTelephone;
	}
	public void setExIsActive(Boolean exIsActive) {
		this.exIsActive = exIsActive;
	}
	public void setSingleRoom(String singleRoom) {
		this.singleRoom = singleRoom;
	}
	public void setDoubleRoom(String doubleRoom) {
		this.doubleRoom = doubleRoom;
	}
	public void setTripleRoom(String tripleRoom) {
		this.tripleRoom = tripleRoom;
	}
	public void setUser(Long user) {
		this.user = user;
	}
	@Override
	public String toString() {
		return "HostelRequest [id=" + id + ", exName=" + exName + ", exHostelType=" + exHostelType + ", exHostelCode="
				+ exHostelCode + ", exHostelCapacity=" + exHostelCapacity + ", exHostelIncharge=" + exHostelIncharge
				+ ", exHostelAddress=" + exHostelAddress + ", colleges=" + colleges + ", exTelephone=" + exTelephone
				+ ", exIsActive=" + exIsActive + ", singleRoom=" + singleRoom + ", doubleRoom=" + doubleRoom
				+ ", tripleRoom=" + tripleRoom + ", user=" + user + "]";
	}
	
	

}
