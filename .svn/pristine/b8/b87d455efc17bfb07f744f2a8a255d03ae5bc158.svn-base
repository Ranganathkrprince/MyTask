package com.niftem.app.model;

import java.util.List;

import javax.validation.constraints.NotNull;

public class TeacherCourseMappingRequest {
	
	private Long id;
	
	@NotNull(message = "Please provide colleges")
	private Long colleges;
	
	@NotNull(message = "Please provide academicyear")
	private Long academicYear;
	
	@NotNull(message = "Please provide Programmes")
	private Long programmes;
	
	@NotNull(message = "Please provide staff")
	private Long staff;
	
	@NotNull(message = "Please provide StaffType")
	private Long staffType;
	
	@NotNull(message = "Please select courseType")
	private List<Long> courseType;
	
	@NotNull(message = "Please select section")
	private Long section;

	@NotNull(message = "Please select courses")
	private List<Long> courses;
	
	@NotNull(message = "Please provide a exIsActive 0/1")
	private Boolean exIsActive;
	
	@NotNull(message = "Please provide Programmeyear")
	private Long programmeYear;
	
	@NotNull(message = "Please provide Semester")
	private Long semester;
	
	@NotNull(message = "Please provide Degree")
	private Long degree;

    private Long stream;
    
	@NotNull(message = "Please provide Department")
	private Long department;
	@NotNull(message = "Please provide registrationType")
	private Long registrationType;
	
	private Long user;

	public Long getId() {
		return id;
	}

	public Long getColleges() {
		return colleges;
	}

	public Long getAcademicYear() {
		return academicYear;
	}

	public Long getProgrammes() {
		return programmes;
	}

	public Long getStaff() {
		return staff;
	}

	public Long getStaffType() {
		return staffType;
	}

	public List<Long> getCourseType() {
		return courseType;
	}

	public Long getSection() {
		return section;
	}

	public List<Long> getCourses() {
		return courses;
	}

	public Boolean getExIsActive() {
		return exIsActive;
	}

	public Long getProgrammeYear() {
		return programmeYear;
	}

	public Long getSemester() {
		return semester;
	}

	public Long getDegree() {
		return degree;
	}

	public Long getStream() {
		return stream;
	}

	public Long getDepartment() {
		return department;
	}

	public Long getRegistrationType() {
		return registrationType;
	}

	public Long getUser() {
		return user;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public void setColleges(Long colleges) {
		this.colleges = colleges;
	}

	public void setAcademicYear(Long academicYear) {
		this.academicYear = academicYear;
	}

	public void setProgrammes(Long programmes) {
		this.programmes = programmes;
	}

	public void setStaff(Long staff) {
		this.staff = staff;
	}

	public void setStaffType(Long staffType) {
		this.staffType = staffType;
	}

	public void setCourseType(List<Long> courseType) {
		this.courseType = courseType;
	}

	public void setSection(Long section) {
		this.section = section;
	}

	public void setCourses(List<Long> courses) {
		this.courses = courses;
	}

	public void setExIsActive(Boolean exIsActive) {
		this.exIsActive = exIsActive;
	}

	public void setProgrammeYear(Long programmeYear) {
		this.programmeYear = programmeYear;
	}

	public void setSemester(Long semester) {
		this.semester = semester;
	}

	public void setDegree(Long degree) {
		this.degree = degree;
	}

	public void setStream(Long stream) {
		this.stream = stream;
	}

	public void setDepartment(Long department) {
		this.department = department;
	}

	public void setRegistrationType(Long registrationType) {
		this.registrationType = registrationType;
	}

	public void setUser(Long user) {
		this.user = user;
	}

	@Override
	public String toString() {
		return "TeacherCourseMappingRequest [id=" + id + ", colleges=" + colleges + ", academicYear=" + academicYear
				+ ", programmes=" + programmes + ", staff=" + staff + ", staffType=" + staffType + ", courseType="
				+ courseType + ", section=" + section + ", courses=" + courses + ", exIsActive=" + exIsActive
				+ ", programmeYear=" + programmeYear + ", semester=" + semester + ", degree=" + degree + ", stream="
				+ stream + ", department=" + department + ", registrationType=" + registrationType + ", user=" + user
				+ "]";
	}
