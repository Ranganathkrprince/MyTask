package com.niftem.app.model;

import java.util.List;

import javax.validation.constraints.NotNull;

public class InvigilatorAllocationRequest {
	
	private Long id;
	@NotNull(message = "colleges is required")
	private Long colleges;
	@NotNull(message = "academicYear is required")
	private Long academicYear;	
	@NotNull(message = "staff is required")
	private Long staff;
	@NotNull(message = "staffType is required")
	private Long StaffType;
	@NotNull(message = "examAnnouncement is required")
	private Long examAnnouncement;
	@NotNull(message = "department is required")
	private Long department;
	@NotNull(message = "programmes is required")
	private Long programmes;
	
	@NotNull(message = "degree is required")
	private Long degree;	
	@NotNull(message = "Please provide a exIsActive 0/1")
	private Boolean exIsActive;
	private Long user;
	@NotNull(message = "startDate is required")
	private List exStartDate; 
	@NotNull(message = "StartTime is required")
	private String exStartTime;
	@NotNull(message = "EndTime is required")
	private String exEndTime;
	
	private Long roomId;

	public Long getId() {
		return id;
	}

	public Long getColleges() {
		return colleges;
	}

	public Long getAcademicYear() {
		return academicYear;
	}

	public Long getStaff() {
		return staff;
	}

	public Long getStaffType() {
		return StaffType;
	}

	public Long getExamAnnouncement() {
		return examAnnouncement;
	}

	public Long getDepartment() {
		return department;
	}

	public Long getProgrammes() {
		return programmes;
	}

	public Long getDegree() {
		return degree;
	}

	public Boolean getExIsActive() {
		return exIsActive;
	}

	public Long getUser() {
		return user;
	}

	public List getExStartDate() {
		return exStartDate;
	}

	public String getExStartTime() {
		return exStartTime;
	}

	public String getExEndTime() {
		return exEndTime;
	}

	public Long getRoomId() {
		return roomId;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public void setColleges(Long colleges) {
		this.colleges = colleges;
	}

	public void setAcademicYear(Long academicYear) {
		this.academicYear = academicYear;
	}

	public void setStaff(Long staff) {
		this.staff = staff;
	}

	public void setStaffType(Long staffType) {
		StaffType = staffType;
	}

	public void setExamAnnouncement(Long examAnnouncement) {
		this.examAnnouncement = examAnnouncement;
	}

	public void setDepartment(Long department) {
		this.department = department;
	}

	public void setProgrammes(Long programmes) {
		this.programmes = programmes;
	}

	public void setDegree(Long degree) {
		this.degree = degree;
	}

	public void setExIsActive(Boolean exIsActive) {
		this.exIsActive = exIsActive;
	}

	public void setUser(Long user) {
		this.user = user;
	}

	public void setExStartDate(List exStartDate) {
		this.exStartDate = exStartDate;
	}

	public void setExStartTime(String exStartTime) {
		this.exStartTime = exStartTime;
	}

	public void setExEndTime(String exEndTime) {
		this.exEndTime = exEndTime;
	}

	public void setRoomId(Long roomId) {
		this.roomId = roomId;
	}

	@Override
	public String toString() {
		return "InvigilatorAllocationRequest [id=" + id + ", colleges=" + colleges + ", academicYear=" + academicYear
				+ ", staff=" + staff + ", StaffType=" + StaffType + ", examAnnouncement=" + examAnnouncement
				+ ", department=" + department + ", programmes=" + programmes + ", degree=" + degree + ", exIsActive="
				+ exIsActive + ", user=" + user + ", exStartDate=" + exStartDate + ", exStartTime=" + exStartTime
				+ ", exEndTime=" + exEndTime + ", roomId=" + roomId + "]";
	}
	

}
