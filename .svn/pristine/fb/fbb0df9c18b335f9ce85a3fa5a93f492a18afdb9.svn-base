package com.niftem.app.model;

public class RoleRequest {
	
	private Long id;

    private String exRoleName;
    
	private Boolean exIsActive;

	private Boolean exIsDelete;
	
	private Long user;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getExRoleName() {
		return exRoleName;
	}

	public void setExRoleName(String exRoleName) {
		this.exRoleName = exRoleName;
	}

	public Boolean getExIsActive() {
		return exIsActive;
	}

	public void setExIsActive(Boolean exIsActive) {
		this.exIsActive = exIsActive;
	}

	public Boolean getExIsDelete() {
		return exIsDelete;
	}

	public void setExIsDelete(Boolean exIsDelete) {
		this.exIsDelete = exIsDelete;
	}

	public Long getUser() {
		return user;
	}

	public void setUser(Long user) {
		this.user = user;
	}

	@Override
	public String toString() {
		return "RoleRequest [id=" + id + ", exRoleName=" + exRoleName + ", exIsActive=" + exIsActive + ", exIsDelete="
				+ exIsDelete + ", user=" + user + "]";
	}



}
